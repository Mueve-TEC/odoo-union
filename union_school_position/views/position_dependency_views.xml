<odoo>
    <data>
        <record model="ir.ui.view" id="union_school_position_dependency_form">
            <field name="name">School position dependency form</field>
            <field name="model">school_position.dependency</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />
                            <field name="code" />
                            <field name="parent_id" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="union_school_position_dependency_tree">
            <field name="name">School position dependency tree</field>
            <field name="model">school_position.dependency</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <field name="code" />
                    <field name="parent_id" />
                </tree>
            </field>
        </record>
    </data>
</odoo>