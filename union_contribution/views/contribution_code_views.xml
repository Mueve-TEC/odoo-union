<odoo>
    <data>
        <record model="ir.ui.view" id="union_contribution_affiliate_contribution_code_form">
            <field name="name">union_contribution.affiliate.contribution.code.form</field>
            <field name="model">contribution.affiliate_contribution_code</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group name="Information">
                            <field name="code" />
                            <field name="name" />
                            <field name="enabled" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="union_contribution_affiliate_contribution_code_tree">
            <field name="name">union_contribution.affiliate.contribution.code.tree</field>
            <field name="model">contribution.affiliate_contribution_code</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code" />
                    <field name="name" />
                    <field name="enabled" />
                </tree>
            </field>
        </record>
    </data>
</odoo>